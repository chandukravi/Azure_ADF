{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Load-ADL2-SQLDB"
		},
		"ADLGen2_LinkedSer_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLGen2_LinkedSer'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"ADLGen2_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ADLGen2-Keyvault.vault.azure.net/"
		},
		"ADLGen2_KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ADLGen2-Keyvault.vault.azure.net/"
		},
		"ADLGen2_LinkedSer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ckblob.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLGen2_LinkedSer_accountKey": {
						"value": "[parameters('ADLGen2_LinkedSer_accountKey')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"ADLGen2_properties_typeProperties_baseUrl": {
						"value": "[parameters('ADLGen2_properties_typeProperties_baseUrl')]"
					},
					"ADLGen2_KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ADLGen2_KeyVault_properties_typeProperties_baseUrl')]"
					},
					"ADLGen2_LinkedSer_properties_typeProperties_url": {
						"value": "[parameters('ADLGen2_LinkedSer_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}